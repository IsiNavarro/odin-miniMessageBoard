<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel="stylesheet" href="/stylesheets/style.css" />
  </head>
  <body>
    <h1><%= title %></h1>
    <ul>
      <% messages.forEach(message => { %>
      <li>
        <h3><%= message.user %></h3>
        <p><%= message.added %></p>
        <p><%= message.text %></p>
      </li>

      <%})%>
    </ul>
  </body>
</html>
